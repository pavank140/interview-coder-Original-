{
  "name": "refractor",
  "version": "3.6.0",
  "description": "Lightweight, robust, elegant virtual syntax highlighting using Prism",
  "license": "MIT",
  "repository": "wooorm/refractor",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/wooorm"
  },
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "files": [
    "index.js",
    "core.js",
    "lang"
  ],
  "dependencies": {
    "hastscript": "^6.0.0",
    "parse-entities": "^2.0.0",
    "prismjs": "~1.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "alpha-sort": "^4.0.0",
    "arr-diff": "^4.0.0",
    "async": "^3.0.0",
    "bail": "^1.0.0",
    "browserify": "^17.0.0",
    "chalk": "^4.0.0",
    "detab": "^2.0.0",
    "is-hidden": "^1.0.0",
    "is-keyword": "^1.2.2",
    "mdast-zone": "^4.0.0",
    "not": "^0.1.0",
    "nyc": "^15.0.0",
    "prettier": "^2.0.0",
    "rehype": "^11.0.0",
    "remark-cli": "^9.0.0",
    "remark-preset-wooorm": "^8.0.0",
    "tape": "^5.0.0",
    "tinyify": "^3.0.0",
    "trim-lines": "^1.0.0",
    "unist-builder": "^2.0.0",
    "unist-util-remove-position": "^3.0.0",
    "xo": "^0.37.0"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm",
      [
        "toc",
        {
          "heading": "contents",
          "skip": "license"
        }
      ],
      "./script/count",
      "./script/support"
    ]
  }
}